{
    "manifest_version": 3,
    "name": "Chroma Color - Chrome",
    "version": "1.3",
    "description": "Get some design inspiration from any web page!",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
      "128": "./logo.png"
    }
    },
    "icons": {
      "128": "./logo.png"
    }
  }